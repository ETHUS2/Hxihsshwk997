local OrionLib = loadstring(game:HttpGet('https://raw.githubusercontent.com/NIGHT163626/ScriptHUB/refs/heads/main/open%20sourcezada.txt'))()
local Player = game.Players.LocalPlayer

local Window = OrionLib:MakeWindow({
    Name = "Xethus Hub | Universal BR", 
    SearchBar = { Default = 'Pesquise', ClearTextOnFocus = true }, 
    HidePremium = false, 
    SaveConfig = true, 
    ConfigFolder = "Xethus",
    IntroText = "Xethus Hub",
    IntroIcon = "rbxassetid://73200509058998"
})

-- Função Auxiliar para Adicionar Botões
local function AddScriptButton(tab, name, url)
    tab:AddButton({
        Name = name,
        Callback = function()
            local ChatService = game:GetService("Chat")
            if Player.Character and Player.Character:FindFirstChild("Head") then
                ChatService:Chat(Player.Character.Head, "Executando...", Enum.ChatColor.White)
            end
            loadstring(game:HttpGet(url))()
        end
    })
end

-- [ Aba: Painel ]
local PainelTab = Window:MakeTab({
    Name = "Painel",
    Icon = "rbxassetid://11819163504",
    PremiumOnly = false
})

-- Adicionar Scripts (Painel)
AddScriptButton(PainelTab, "</> Aimbot Universal", "https://raw.githubusercontent.com/Yousuck780/troll-x/main/ahhhh")
